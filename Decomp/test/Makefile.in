all: test

test:
	cd ../examples/MILP; make clean; make;

#Data/Sample/pack1 optimal=2 (solve with cutting plane method)
	../examples/MILP/decomp_milp --doPriceCut 0 --doCut 1 --MILP:DataDir ../../../Data/Sample --MILP:Instance pack1 --MILP:BestKnownLB 2 --MILP:BestKnownUB 2
#Data/Sample/pack1 optimal=2 (solve with price and cut method)
	../examples/MILP/decomp_milp --doPriceCut 1 --doCut 0 --MILP:DataDir ../../../Data/Sample --MILP:Instance pack1 --MILP:BestKnownLB 2 --MILP:BestKnownUB 2

#Data/Sample/tp3 optimal=2 (solve with cutting plane method)
	../examples/MILP/decomp_milp --doPriceCut 0 --doCut 1 --MILP:DataDir ../../../Data/Sample --MILP:Instance tp3 --MILP:BestKnownLB 155 --MILP:BestKnownUB 155
#Data/Sample/tp3 optimal=2 (solve with price and cut method)
	../examples/MILP/decomp_milp --doPriceCut 1 --doCut 0 --MILP:DataDir ../../../Data/Sample --MILP:Instance tp3 --MILP:BestKnownLB 155 --MILP:BestKnownUB 155

clean:
	cd ../examples/MILP; make clean;


